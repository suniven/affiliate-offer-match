   "source": [
    "import pandas as pd\n",
    "\n",
    "with open('./query.txt', 'r') as f:\n",
    "    list=f.readlines()\n",
    "keyword=[]\n",
    "for domain in list:\n",
    "    a = domain.strip().split('.')[-2]\n",
    "    if len(a)>2:\n",
    "        keyword.append(a)\n",
    "df = pd.DataFrame(keyword, columns=[\"keyword\"])\n",
    "df.to_csv('./keyword.csv', index=False)"
   ]
  }
